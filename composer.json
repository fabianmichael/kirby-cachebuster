{
  "name": "fabianmichael/kirby-cachebuster",
  "description": "Cachebusting for CSS and JS files in Kirby.",
  "type": "kirby-plugin",
  "version": "1.0.1",
  "require": {
    "getkirby/composer-installer": "^1.2"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Fabian Michael",
      "email": "hallo@fabianmichael.de"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
    "installer-name": "cachebuster"
  }
}
